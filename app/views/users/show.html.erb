<h1>Users#show</h1>
<p>This is the Show view for User</p>

<h2>Attended Events</h2>
<% @current_user.attended_events.each do |event| %>
  <%= event.name %>
<% end %>
<h2>Previous Events</h2>
<% @current_user.previous_events.each do |event| %>
  <%= event.name %>
<% end %>
<h2>Upcoming Events</h2>
<% @current_user.upcoming_events.each do |event| %>
  <%= event.name %>
<% end %> 